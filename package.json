{
	"name": "discord-bot",
	"version": "1.1.0",
	"keywords": [
		"bot",
		"discord",
		"ut",
		"unreal",
		"unrealtournament",
		"pickup",
		"pug"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"main": "lib/index.js",
	"module": "ES2022",
	"type": "module",
	"scripts": {
		"start": "node lib/index.js --env public",
		"dev:start": "node lib/index.js --env development --config conf/config.json",
		"debug": "nodemon --ignore db/* --ignore web/page.html --ignore conf/config_live.json --ignore conf/persistent.json --exec npm run dev:start",
		"lint": "eslint ."
	},
	"dependencies": {
		"commander": "^9.4.1",
		"discord.js": "^14.6.0",
		"irc-colors": "^1.5.0",
		"irc-formatting": "^1.0.0-rc3",
		"irc-upd": "^0.11.0",
		"lodash": "^4.17.21",
		"simple-markdown": "^0.7.3",
		"sqlite3": "^5.1.7",
		"strip-json-comments": "^5.0.0",
		"winston": "^3.8.2"
	},
	"devDependencies": {
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1"
	}
}
